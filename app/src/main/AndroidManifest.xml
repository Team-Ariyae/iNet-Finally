        <activity
            android:permission="app.openconnect.REMOTE_API"
            android:name="app.openconnect.api.GrantPermissionsActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
